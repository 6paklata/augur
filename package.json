{
  "name": "augur-node",
  "version": "0.1.0",
  "description": "Augur Node",
  "author": "The Augur Developers <team@augur.net>",
  "license": "MIT",
  "main": "build/index.js",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "test": "mocha test",
    "lint": "tslint -c tslint.json -p .",
    "build": "tsc",
    "migrate": "node node_modules/knex/bin/cli.js migrate:latest --env build",
    "seed": "node node_modules/knex/bin/cli.js seed:run --env build",
    "migrate:make": "node node_modules/knex/bin/cli.js migrate:make -x ts $1",
    "coverage": "istanbul cover --include-all-sources --root ./build ./node_modules/mocha/bin/_mocha test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AugurProject/augur-node.git"
  },
  "bugs": {
    "url": "https://github.com/AugurProject/augur-node/issues"
  },
  "homepage": "https://github.com/AugurProject/augur-node#readme",
  "dependencies": {
    "@types/knex": "0.0.61",
    "@types/lodash": "4.14.77",
    "async": "1.5.2",
    "augur.js": "4.4.6",
    "bignumber.js": "4.1.0",
    "knex": "0.13.0",
    "lodash.isnull": "3.0.0",
    "lodash.omit": "4.5.0",
    "pg": "7.3.0",
    "sqlite3": "3.1.8",
    "ws": "3.2.0"
  },
  "devDependencies": {
    "@types/async": "2.0.43",
    "@types/bignumber.js": "4.0.3",
    "@types/node": "8.0.31",
    "@types/sqlite3": "3.1.1",
    "@types/ws": "3.0.2",
    "chai": "3.5.0",
    "coveralls": "2.11.3",
    "eslint": "3.17.1",
    "istanbul": "0.4.1",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "0.0.2",
    "proxyquire": "1.7.11",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "2.5.2"
  }
}
