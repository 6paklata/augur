  "bugs": {
    "url": "https://github.com/AugurProject/augur.js/issues"
  },
  "homepage": "https://github.com/AugurProject/augur.js",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-loose": "^6.1.3",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^3.0.0",
    "coveralls": "^2.11.3",
    "del": "^1.2.0",
    "geth": "^0.1.4",
    "gulp": "^3.9.0",
    "gulp-uglify": "^1.5.1",
    "istanbul": "^0.3.17",
    "it-each": "^0.3.1",
