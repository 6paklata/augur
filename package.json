    "longjohn": "^0.2.9",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2",
    "posix-getopt": "^1.1.0",
    "rimraf": "^2.4.0",
    "uglify-js": "^2.6.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "web3": "^0.8.1"
  },
  "author": "Jack Peterson <jack@tinybike.net>",
