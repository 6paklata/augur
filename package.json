{
  "name": "augur.js",
  "version": "0.12.21",
  "description": "Augur JavaScript API",
  "main": "src/index.js",
  "scripts": {
    "test": "scripts/runtests.sh --connect --core --compat --create --markets --client",
    "lint": "scripts/runtests.sh",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha test/core test/markets test/client --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "testnet": "scripts/runtests.sh --connect --core --compat --markets --client",
    "markets": "scripts/setup.js --geth --contract branches && mocha test/create/createEvent.js && mocha test/markets/trading.js && mocha test/markets/markets.js",
    "timing": "scripts/timing.js && scripts/timing.jl && gpicview data/timing-getMarketsInfo.png",
    "setup": "scripts/setup.js --reset"
  },
  "dependencies": {
    "async": "^1.3.0",
    "augur-abi": "^0.2.7",
    "augur-contracts": "^0.2.5",
    "bignumber.js": "^2.0.7",
    "buffer": "^3.5.3",
    "chalk": "^1.1.1",
    "crypto-browserify": "^3.9.14",
    "ethereumjs-tx": "^0.6.9",
    "ethereumjs-util": "^1.5.1",
    "ethrpc": "^0.4.8",
    "ipfs-api": "^2.9.11",
    "js-sha3": "^0.3.1",
    "keythereum": "^0.2.0",
    "moment": "^2.10.3",
    "multi-hash": "^0.1.1",
    "node-localstorage": "^0.6.0",
    "node-uuid": "^1.4.3",
    "validator": "^4.0.2"
  },
  "browser": {
    "ipfs-api": false
  },
  "engines": {
    "node": ">= 4.0.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AugurProject/augur.js.git"
